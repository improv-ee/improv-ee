{
    "name": "improv-ee/improv-web",
    "description": "Improvised theatre schedule and info",
    "keywords": ["improvisation", "theatre"],
    "homepage": "https://github.com/improv-ee/improv-web",
    "license": "Apache-2.0",
    "type": "project",
    "require": {
        "php": "^7.2.17",
        "akaunting/language": "^1.0.13",
        "askedio/laravel-soft-cascade": "^6.0",
        "astrotomic/laravel-translatable": "^11.6",
        "clarkeash/doorman": "^4.0",
        "dirape/token": "^2.1",
        "fideloper/proxy": "^4.0",
        "joggapp/laravel-google-translate": "^3.0.0",
        "laravel/framework": "6.5.0",
        "laravel/passport": "^8.0",
        "laravel/tinker": "^1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "mpociot/laravel-apidoc-generator": "^3.17",
        "owen-it/laravel-auditing": "^9.3",
        "sentry/sentry-laravel": "^1.4",
        "skagarwal/google-places-api": "^1.5",
        "spatie/laravel-backup": "^6.7",
        "spatie/laravel-cors": "^1.6",
        "spatie/laravel-csp": "^2.4",
        "spatie/laravel-http-logger": "^1.3",
        "spatie/laravel-medialibrary": "^7.14",
        "spatie/laravel-permission": "^3.2",
        "spatie/laravel-query-builder": "^2.3",
        "spatie/laravel-sluggable": "^2.2.1",
        "spatie/laravel-tags": "^2.5",
        "thomaswelton/laravel-gravatar": "^1.2",
        "valorin/pwned-validator": "^1.3",
        "mastani/laravel-google-static-map": "dev-master as 2.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.8",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^8.4",
        "jakub-onderka/php-parallel-lint":"^1",
        "doctrine/dbal":"^2.9"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mastani/laravel-google-static-map"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
